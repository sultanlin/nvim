{
  "if": {
    "prefix": "if",
    "body": "if ($1) {\n\t$2\n}$0",
    "description": "if"
  },
  "ife": {
    "prefix": "ife",
    "body": "if ($1) {\n\t$2\n} else {\n\t$3\n}$0",
    "description": "if-else"
  },
  "eif": {
    "prefix": "eif",
    "body": "else if ($1) {\n\t$2\n}$0",
    "description": "else-if"
  },
  "el": {
    "prefix": "el",
    "body": "else {\n\t$1\n}$0",
    "description": "else"
  },
  "function": {
    "prefix": "fun",
    "body": "function($1) {\n\t$2\n}$0",
    "description": "Function"
  },
  "for": {
    "prefix": "for",
    "body": "for (${1:item} in ${2:list}) {\n\t$3\n}$0",
    "description": "for loop"
  },
  "wh": {
    "prefix": "wh",
    "body": "while ($1) {\n\t$2\n}$0",
    "description": "while loop"
  },
  "lib": {
    "prefix": "lib",
    "body": "library(\"${1:package}\")$0",
    "description": "Import a library"
  },
  "req": {
    "prefix": "req",
    "body": "require(\"${1:package}\")$0",
    "description": "Require a file"
  },
  "ret": {
    "prefix": "ret",
    "body": "return($1)$0",
    "description": "Return call"
  },
  "testthat": {
    "prefix": "test_",
    "body": "test_that(\"${1:}\", {\n\t$2\n})$0",
    "description": "Add test"
  },
  "ggplot": {
    "prefix": "ggplot",
    "body": "ggplot(${1:}aes(${2:})) +\ngeom_${3:}",
    "description": "Start ggplot"
  }
}
